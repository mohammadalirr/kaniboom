    <script>
      const form = document.querySelector(".masir-form");
      const howSelect = document.querySelector("#how");
      const applicantSelect = document.querySelector("#applicant");
      const studentField = document.querySelector("#student-field");
      const companyField = document.querySelector("#company-field");
      const studentInput = document.querySelector(".student-input");
      const companyInput = document.querySelectorAll(".company-input");

      applicantSelect.oninput = (e) => {
        if (e.target.value === "دانشجو/صاحب ایده") {
          studentField.style.display = "block";
          companyField.style.display = "none";
          studentInput.setAttribute("required", "");
          companyInput.forEach((i) => {
            i.removeAttribute("required");
          });
        } else if (e.target.value === "شرکت/استارتاپ") {
          studentField.style.display = "none";
          companyField.style.display = "block";
          studentInput.removeAttribute("required");
          companyInput.forEach((i) => {
            i.setAttribute("required", "");
          });
        } else {
          studentField.style.display = "none";
          companyField.style.display = "none";
          studentInput.removeAttribute("required");
          companyInput.forEach((i) => {
            i.removeAttribute("required");
          });
        }
      };

      form.onsubmit = (e) => {
        e.preventDefault();
        //submitForm function in interface.js
        submitForm(form, "masir");
      };
    </script>
