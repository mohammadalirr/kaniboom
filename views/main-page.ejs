    <!-- Layout -->

    <div class="layout">
      <!-- Home -->

      <main class="main">
        <div class="arrow-left"></div>
        <div class="arrow-right"></div>

        <!-- Start revolution slider -->

        <div class="rev_slider_wrapper">
          <div id="rev_slider" class="rev_slider" style="max-height: 100%; margin: 0 auto">
            <ul>
              <% if (pageData.slides[0]) { %>
                <% pageData.slides.forEach((i, index) => { %>

                  <!-- Slide -1 -->

                     <li
                       data-transition="slotzoom-horizontal"
                       data-slotamount="7"
                       data-easein="Power3.easeInOut"
                       data-easeout="Power3.easeInOut"
                       data-masterspeed="1000"
                     >

                       <!-- conmine slide -->
                       <img
                       src="<%=BASE_URL + "/contents/imgs/" + i.img %>"
                         alt=""
                         data-bgposition="center center"
                         data-bgfit="cover"
                         data-bgrepeat="no-repeat"
                         class="rev-slidebg"
                       />
                       <div
                         style="
                           position: absolute;
                           width: 100%;
                           height: 100px;
                           background-color: rgba(0, 0, 0, 0.3);
                           z-index: 100;
                           transition: all 0.3s;
                         "
                       ></div>

   
                       <div class="section-content btn-slider">
                         <a target="_blank" href=<%=i.link%>
                           ><p class="btn btn-shadow-2">مشاهده</p></a
                         >
                       </div>
                     </li>
                <% }) %>
              <% } else { %> <%null%> <% } %>

              <!-- Slide 0 -->
            </ul>
          </div>
        </div>
      </main>

      <div class="content">
        <!-- About  -->

        <% if (pageData.stats) { %>

          <section id="about" class="about section">
            <div class="container">
              <header class="section-header">
                <h2 class="section-title">
                  درباره <span class="text-primary">کانی بوم</span>
                </h2>
                <strong class="fade-title-left">About</strong>
              </header>
              <div class="section-content">
                <div class="row-base row">
                  <div class="col-base col-sm-12 col-md-4" style="float: right">
                    <div class="col-about-info">
                      <h3
                        class="col-about-title text-right"
                        style="direction: rtl"
                      >
                        <span
                          class="text-primary"
                          style="display: block; padding-bottom: 10px"
                        >
                          کانی بوم</span
                        >
                        شتابدهنده تخصصی حوزه معدن و صنایع معدنی
                      </h3>
                      <p
                        class="text-justify"
                        style="font-size: 14px; padding-left: 0px; direction: rtl"
                      >
                        <%=pageData.intro%>
                      </p>
                    </div>
                  </div>
                  <div
                    class="col-base col-sm-12 col-md-8"
                    style="direction: rtl; float: right; display: flex"
