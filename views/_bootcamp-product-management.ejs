    <script>
      const form = document.querySelector(".product-form");
      const recCheck = document.querySelector("#is_recomended");
      const orgCheck = document.querySelector("#is_org");
      const recField = document.querySelector("#rec-field");
      const orgField = document.querySelector("#org-field");
      const recInputs = document.querySelector(".rec-input");
      const orgInputs = document.querySelectorAll(".org-input");
      recCheck.oninput = () => {
        if (recCheck.checked) {
          recField.style.display = "block";
          recInputs.setAttribute("required", "");
        } else {
          recField.style.display = "none";
          recInputs.removeAttribute("required");
        }
      };
      orgCheck.oninput = () => {
        if (orgCheck.checked) {
          orgField.style.display = "block";
          orgInputs.forEach((i) => {
            i.setAttribute("required", "");
          });
        } else {
          orgField.style.display = "none";
          orgInputs.forEach((i) => {
            i.removeAttribute("required");
          });
        }
      };
      form.onsubmit = (e) => {
        e.preventDefault();
        //submitForm function in interface.js
        submitForm(form, "product");
      };
    </script>
